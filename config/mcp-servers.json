{
  "servers": [
    {
      "id": "github",
      "name": "GitHub MCP Server",
      "description": "Access to GitHub API via Model Context Protocol",
      "transport": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_TOKEN": "${GITHUB_TOKEN}"
      },
      "enabled": true
    },
    {
      "id": "filesystem",
      "name": "Filesystem Server",
      "description": "Access to local filesystem",
      "transport": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/tmp"],
      "enabled": false
    },
    {
      "id": "weather",
      "name": "Weather Server",
      "description": "Access to weather API",
      "transport": "stdio",
      "command": "/Volumes/Data/Projects/MobileDeveloper/Projects/mcpServers/localMcp/weatherMCP/run-mcp-server.sh",
      "args": [],
      "enabled": true
    },
    {
      "id": "Trello",
      "name": "Trello Server",
      "description": "Access to Trello API",
      "transport": "stdio",
      "command": "/Volumes/Data/Projects/MobileDeveloper/Projects/mcpServers/localMcp/trelloMCP/run-mcp-server.sh",
      "args": [],
      "enabled": true
    }
  ]
}
